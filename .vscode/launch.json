   "version": "0.2.0",
   "configurations": [
      {
         "type": "node",
         "request": "launch",
         "name": "Launch Program",
         "skipFiles": [
            "<node_internals>/**"
         ],
         "program": "${workspaceFolder}/server/src/chat/chat.gateway.ts",
         "outFiles": [
            "${workspaceFolder}/**/*.js"
         ]
      },
      {
         "type": "node",
         "request": "launch",
         "name": "Launch via NPM",
         "runtimeExecutable": "npm",
         "runtimeVersion": "18.16.0",
         "cwd": "${workspaceFolder}/server",
         "runtimeArgs": [
           "run",
           "start:debug"
         ],
         "skipFiles": [
           "<node_internals>/**"
         ],
         "outputCapture": "std",
       },
   ]
}